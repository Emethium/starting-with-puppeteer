{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "meta": {
      "title": "Starting with Puppeteer",
      "description": "A Puppeteer introductory repository to be used as reference or inspiration"
    },
    "search": true,
    "collapse": false,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "GitLab repo": {
        "href": "https://github.com/Emethium/starting-with-puppeteer.git",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  }
}
